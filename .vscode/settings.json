{
  "deno.enable": true,
  "deno.unstable": false,
  "deno.suggest.imports.autoDiscover": true,
  "deno.lint": true,
  "eslint.enable": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true
  }
}